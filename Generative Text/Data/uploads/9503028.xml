         <NarrativeText>&lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;CASE NUMBER: # 23-MSP-011044&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;&lt;span style="background-color:#f3f3f3"&gt;INCIDENT RECORDED: &lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;In-car (Marked Q-23) and Body Camera&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;HANDGUN ON PERSON / CDS PWID MARIHUANA&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;&lt;span style="background-color:#f3f3f3"&gt;SCENE PROCESSING:&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Handgun located on drivers&amp;#39; person, prior to vehicle P/C Search for odor of Marihuana.&amp;nbsp; Marihuana and packaging material located in the vehicle.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;&lt;span style="background-color:#f3f3f3"&gt;SUSPECT:&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Uchechi Denning Uzoma&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;08/01/2001&lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;11505 35&lt;sup&gt;th&lt;/sup&gt; Avenue&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Beltsville, MD 20705&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;MD License # MD10272153477&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;&lt;span style="background-color:#f3f3f3"&gt;OFFICER:&lt;/span&gt;&lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;State of Maryland&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;Trooper. B. Peris #7511,&lt;/span&gt;&lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;(301) 345-3101,&lt;/span&gt;&lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;10100 Rhode Island Ave, College Park, MD 20740&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;VICTIM:&lt;/span&gt;&lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;State of Maryland&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;PROPERTY:&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Property Held CDS # P-341153&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;&lt;span style="background-color:#f3f3f3"&gt;Property Held Gun # P-341154&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;&lt;span style="background-color:#f3f3f3"&gt;CHARGES:&lt;/span&gt;&lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;Criminal: Tracking # 170001311723&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;CR 4-203(a)(1)(v) Handgun in vehicle.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;CR 4-203(a)(1)(v) Wear and carry handgun in vehicle.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;CR 4-203 Wear and Carry handgun on person&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;CR 5-602(b) Possession of cannabis in sufficient quantity w/ intent to distribute.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Traffic Violation:&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;13-401(h) Suspended registration. Citation # 0BK0YJG&lt;br /&gt; &lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;NARRATIVE:&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;On Tuesday, March 28, 2023, at approximately 0514 hours, I, Tpr. B. Peris was on routine uniform patrol in the area Sunny Side Ave and Tucker Road Beltsville, Prince George&amp;#39;s County, Maryland.&amp;nbsp; I was in full uniform operating a marked patrol vehicle (Q-23) equipped with an in-car camera.&amp;nbsp; I was also equipped with a body camera.&amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;At the above listed date and time, I was behind a vehicle later identified as a black 2010 Audi passenger car displaying Maryland registration 9EK1049.&amp;nbsp; While behind the vehicle on Edmonston Road prior to Sunny Side Ave, I noticed the vehicle to be swerving in its lane and continuously braking for no reason.&amp;nbsp; Based off these observations, I suspected the driver of possibly being impaired by drugs/alcohol.&amp;nbsp; During a routine check through MVA, the vehicle registration plates were found to be suspended.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;I activated my emergency equipment and the vehicle came to a stop on Sunny Side Ave, west of Tucker Road, in the right lane.&amp;nbsp; I approached the vehicle and made contact with the driver and two other occupants.&amp;nbsp; The driver was identified by his Maryland drivers license as, Uchechi Denning Uzoma.&amp;nbsp; He was also found to be the owner of the vehicle.&amp;nbsp; The driver was informed of who I was, the reason for the stop, and that the stop was being recorded.&amp;nbsp; &lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;While speaking to the driver, I detected a strong odor of both raw and burnt Marihuana emanating from inside the vehicle.&amp;nbsp; I returned to my vehicle and called for another unit for a probable cause search of the vehicle.&amp;nbsp; The driver was found to be valid through Maryland. &lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;&lt;span style="background-color:#f3f3f3"&gt;When my back-up arrived, I asked the driver to exit the vehicle, and step to the rear of his car.&amp;nbsp; The driver questioned me as to why he was being asked to exit the vehicle, and I told him I needed to speak with him.&amp;nbsp; After exiting, I informed the driver that I smelled Marihuana and there was going to be a search of the vehicle.&amp;nbsp; When asked if he had anything in the vehicle or in him, the driver stated he had a handgun on his person.&amp;nbsp; The driver then stated that gun was in his front right pocket of his sweatpants.&amp;nbsp; I then removed the handgun from his pocket,&amp;nbsp; removed the magazine from the gun, and opened the slide rail.&amp;nbsp; The gun was found to have one 9mm round in the chamber and one 9mm round in the 16 round high capacity magazine.&amp;nbsp; The ammunition was Winchester ball ammo.&amp;nbsp; The handgun was identified as a Glock, model 19, 9mm semi-automatic Serial #BNMN516. &lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;The driver stated that he had applied for his HQL in 2022; however, had not received the paperwork yet.&amp;nbsp; The gun was secured at this time.&amp;nbsp; The two other occupants were removed from the vehicle and found not to be armed with any weapons. &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;During a probable cause search of the vehicle I located the following:&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &amp;nbsp; &lt;ol&gt;  &lt;li style="margin-left:8px"&gt;&lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;&amp;nbsp;Baggie of Marihuana in the drivers side door panel compartment.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;  &lt;li style="margin-left:8px"&gt;&lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Marihuana Cigarette in an ashtray and a Marihuana grinder on the front passenger floor board.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;  &lt;li style="margin-left:8px"&gt;&lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;13 new Ziploc baggies in the truck.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;  &lt;li style="margin-left:8px"&gt;&lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;A large clear heat sealed bag of Marihuana in the left&amp;nbsp;access compartment, in the truck.&amp;nbsp; &lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt; &lt;/ol&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;&lt;span style="background-color:#f3f3f3"&gt;I was able to identify all the previous mentioned items by my training, knowledge, and experience.&amp;nbsp; I am aware of the packaging material used in the distribution of controlled dangerous substances, and that the quantity and packaging material combined is indicative of the distribution of said Marihuana.&amp;nbsp; &lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;All CDS items were seized and later packaged according to Maryland State Police policy.&amp;nbsp; The driver was issued a traffic citation for the suspended registration violation.&amp;nbsp; He was also charged criminally for the handgun and the possession with intent to distribute Cannabis.&amp;nbsp; &lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;&lt;span style="background-color:#f3f3f3"&gt;SFST was conducted on the driver due to him showing signs of impairment.&amp;nbsp; The testing was completed satisfactory at the barrack.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;The Maryland State Police Gun Center was contacted, and the driver was found not to be a prohibited person.&amp;nbsp; The firearm was found to be registered to his brother.&amp;nbsp; A copy of the Gun Center Report will be attached to this case.&amp;nbsp; Uzoma was transported to the Upper Marlboro Detention Center and seen by the Commissioner to determine his release.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Exhibits:&lt;br /&gt; Gun property records&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;CDS Property records&lt;br /&gt; Statement of Probable Cause&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Statement of Charges&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="background-color:#f3f3f3"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Traffic citations&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Evidence photos&lt;br /&gt; Search Report&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="color:black"&gt;Gun Center Report&lt;br /&gt; &lt;br /&gt; &lt;span style="background-color:#f3f3f3"&gt;All above events occurred in Prince George&amp;#39;s County, Maryland.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &lt;br /&gt; &lt;span style="font-size:11pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;&lt;span style="font-size:14.0pt"&gt;&lt;span style="line-height:107%"&gt;&lt;span&gt;Case Status: Closed&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt; &amp;nbsp;</NarrativeText>
